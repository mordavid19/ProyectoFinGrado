RewriteEngine On
RewriteBase /Proyecto_Gym/

# Redirige /login a login.php
RewriteRule ^login$ login.php [L]

# Redirige /registro a register.php (si tienes)
RewriteRule ^registro$ register.php [L]


# Puedes añadir más reglas aquí para otras páginas

# Opcional: fuerza que se quite .php de la URL
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]
